{
    "rules": {
        "align": [
            "arguments",
            "parameters",
            "statements"
        ],
        "ban": true,
        "curly": true,
        "class-name": true,
        "comment-format": [
            true,
            "check-space"
        ],
        "eofline": true,
        "indent": [
            true,
            "spaces"
        ],
        "jsdoc-format": true,
        "member-ordering": [
            "public-before-private",
            "static-before-instance",
            "variables-before-functions"
        ],
        "no-arg": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": true,
        "no-construct": true,
        "no-constructor-vars": true,
        "no-debugger": true,
        "no-duplicate-key": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-inferrable-types": true,
        "no-internal-module": true,
        "no-shadowed-variable": true,
        "no-trailing-whitespace": true,
        "no-unreachable": true,
        "no-unused-expression": true,
        "no-use-before-declare": true,
        "no-var-keyword": true,
        "no-var-requires": true,
        "one-line": [
            true,
            "check-open-brace",
            "check-whitespace",
            "check-catch",
            "check-else",
            "check-finally"
        ],
        "quotemark": [
            true,
            "double"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": true,
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            }
        ],
        "use-strict": [
            "check-function",
            "check-module"
        ],
        "variable-name": [
            true,
            "ban-keywords"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type",
            "check-typecast"
        ]
    }
}