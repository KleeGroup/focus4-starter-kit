{
    "name": "focus4-starter-kit",
    "version": "10.2.0",
    "description": "Starter kit pour Focus V4",
    "repository": "http://www.github.com/get-focus/focus4-starter-kit",
    "main": "src/index.ts",
    "scripts": {
        "build": "rm -rf static/* && tcm src && webpack --mode=production",
        "start": "rm -rf static/* && webpack-dev-server --mode=development --devtool eval-source-map",
        "stylelint": "stylelint src/**/*.css",
        "tcm": "tcm src",
        "tslint": "tslint --project tsconfig.json --force"
    },
    "dependencies": {
        "focus4": "10.2.0"
    },
    "devDependencies": {
        "css-loader": "3.4.2",
        "file-loader": "5.0.2",
        "fork-ts-checker-webpack-plugin": "4.0.4",
        "postcss": "7.0.26",
        "postcss-apply": "0.12.0",
        "postcss-calc": "7.0.1",
        "postcss-color-function": "4.1.0",
        "postcss-custom-properties": "7.0.0",
        "postcss-loader": "3.0.0",
        "postcss-import": "12.0.1",
        "postcss-preset-env": "6.7.0",
        "source-map-loader": "0.2.4",
        "style-loader": "1.1.3",
        "stylelint": "13.2.0",
        "ts-loader": "6.2.1",
        "tslint": "6.0.0",
        "typescript": "3.7.5",
        "typescript-tslint-plugin": "0.5.5",
        "typed-css-modules": "0.3.7",
        "url-loader": "3.0.0",
        "webpack": "4.41.6",
        "webpack-cli": "3.3.11",
        "webpack-dev-server": "3.10.3"
    },
    "author": "KleeGroup",
    "license": "MIT"
}
