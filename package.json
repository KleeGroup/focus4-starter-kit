{
    "name": "focus4-starter-kit",
    "version": "10.8.9",
    "description": "Starter kit pour Focus V4",
    "repository": "http://www.github.com/get-focus/focus4-starter-kit",
    "main": "src/index.ts",
    "scripts": {
        "build": "rm -rf static/* && node cssgen.js && webpack --mode=production",
        "cssgen": "node cssgen.js",
        "start": "rm -rf static/* && webpack serve --mode=development --devtool eval-source-map",
        "stylelint": "stylelint src/**/*.css",
        "tslint": "tslint --project tsconfig.json --force"
    },
    "dependencies": {
        "focus4": "10.8.9"
    },
    "devDependencies": {
        "css-loader": "4.3.0",
        "file-loader": "6.2.0",
        "fork-ts-checker-webpack-plugin": "6.2.12",
        "postcss-apply": "0.12.0",
        "postcss-calc": "7.0.5",
        "postcss-color-function": "4.1.0",
        "postcss-custom-properties": "10.0.0",
        "postcss-import": "12.0.1",
        "postcss-loader": "4.2.0",
        "postcss-preset-env": "6.7.0",
        "source-map-loader": "3.0.0",
        "style-loader": "2.0.0",
        "ts-loader": "9.2.3",
        "tslint": "6.1.3",
        "typescript": "4.3.4",
        "typescript-tslint-plugin": "1.0.1",
        "url-loader": "4.1.1",
        "webpack": "5.40.0",
        "webpack-cli": "4.7.2",
        "webpack-dev-server": "3.11.2"
    },
    "author": "KleeGroup",
    "license": "MIT"
}
