{
    "name": "focus4-starter-kit",
    "version": "10.0.0-alpha.1",
    "description": "Starter kit pour Focus V4",
    "repository": "http://www.github.com/get-focus/focus4-starter-kit",
    "main": "src/index.ts",
    "scripts": {
        "build": "rm -rf static/* && tcm src && webpack --mode=production",
        "start": "rm -rf static/* && webpack-dev-server --mode=development --devtool eval-source-map",
        "stylelint": "stylelint src/**/*.css",
        "tcm": "tcm src",
        "tslint": "tslint --project tsconfig.json --force"
    },
    "dependencies": {
        "@focus4/collections": "10.0.0-alpha.1",
        "@focus4/layout": "10.0.0-alpha.1"
    },
    "devDependencies": {
        "css-loader": "1.0.1",
        "file-loader": "3.0.1",
        "fork-ts-checker-webpack-plugin": "1.3.4",
        "postcss": "7.0.16",
        "postcss-color-function": "4.1.0",
        "postcss-custom-properties": "7.0.0",
        "postcss-import": "12.0.1",
        "postcss-loader": "3.0.0",
        "postcss-preset-env": "6.6.0",
        "source-map-loader": "0.2.4",
        "style-loader": "0.23.1",
        "stylelint": "10.0.1",
        "ts-loader": "6.0.1",
        "tslint": "5.16.0",
        "typescript": "3.5.0-rc",
        "typescript-tslint-plugin": "0.4.0",
        "typed-css-modules": "0.3.7",
        "typings-for-css-modules-loader": "1.7.0",
        "url-loader": "1.1.2",
        "webpack": "4.32.1",
        "webpack-cli": "3.3.2",
        "webpack-dev-server": "3.4.1"
    },
    "author": "KleeGroup",
    "license": "MIT"
}
