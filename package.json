{
    "name": "focus4-starter-kit",
    "version": "10.0.0-beta.1",
    "description": "Starter kit pour Focus V4",
    "repository": "http://www.github.com/get-focus/focus4-starter-kit",
    "main": "src/index.ts",
    "scripts": {
        "build": "rm -rf static/* && tcm src && webpack --mode=production",
        "start": "rm -rf static/* && webpack-dev-server --mode=development --devtool eval-source-map",
        "stylelint": "stylelint src/**/*.css",
        "tcm": "tcm src",
        "tslint": "tslint --project tsconfig.json --force"
    },
    "dependencies": {
        "focus4": "10.0.0-beta.1"
    },
    "devDependencies": {
        "css-loader": "2.1.1",
        "file-loader": "4.0.0",
        "fork-ts-checker-webpack-plugin": "1.3.5",
        "postcss": "7.0.17",
        "postcss-custom-properties": "7.0.0",
        "postcss-loader": "3.0.0",
        "postcss-import": "12.0.1",
        "postcss-preset-env": "6.6.0",
        "source-map-loader": "0.2.4",
        "style-loader": "0.23.1",
        "stylelint": "10.1.0",
        "ts-loader": "6.0.2",
        "tslint": "5.17.0",
        "typescript": "3.5.1",
        "typescript-tslint-plugin": "0.4.0",
        "typed-css-modules": "0.3.7",
        "url-loader": "2.0.0",
        "webpack": "4.33.0",
        "webpack-cli": "3.3.3",
        "webpack-dev-server": "3.7.1"
    },
    "author": "KleeGroup",
    "license": "MIT"
}
