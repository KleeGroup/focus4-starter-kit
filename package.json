{
    "name": "focus4-starter-kit",
    "version": "9.6.2",
    "description": "Starter kit pour Focus V4",
    "repository": "http://www.github.com/get-focus/focus4-starter-kit",
    "main": "src/index.ts",
    "scripts": {
        "build": "rm -rf static/* && tcm src && webpack --mode=production",
        "start": "rm -rf static/* && webpack-dev-server --mode=development --devtool eval-source-map",
        "stylelint": "stylelint src/**/*.css",
        "tcm": "tcm src",
        "tslint": "tslint --project tsconfig.json --force"
    },
    "dependencies": {
        "focus4": "9.6.2"
    },
    "devDependencies": {
        "css-loader": "1.0.0",
        "file-loader": "2.0.0",
        "fork-ts-checker-webpack-plugin": "0.4.10",
        "json-loader": "0.5.7",
        "postcss": "7.0.5",
        "postcss-cssnext": "3.1.0",
        "postcss-import": "12.0.0",
        "postcss-loader": "3.0.0",
        "source-map-loader": "0.2.4",
        "style-loader": "0.23.1",
        "stylelint": "9.6.0",
        "ts-loader": "5.2.2",
        "tslint": "5.11.0",
        "typescript": "3.1.3",
        "typed-css-modules": "0.3.6",
        "typings-for-css-modules-loader": "1.7.0",
        "url-loader": "1.1.2",
        "webpack": "4.21.0",
        "webpack-cli": "3.1.2",
        "webpack-dev-server": "3.1.9"
    },
    "author": "KleeGroup",
    "license": "MIT"
}
